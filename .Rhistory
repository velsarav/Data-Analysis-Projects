summary(air_quality_data)
names(air_quality_data)
ggplot(aes(x = Good.Days), data = air_quality_data) +
geom_histogram(binwidth = 5) +
facet_wrap(~State)
#Good Days
ggplot(aes(Good.Days), data=air_quality_data) +
geom_histogram(binwidth = 5, fill="green", color="black")
ggplot(aes(Moderate.Days), data=air_quality_data) +
geom_histogram(binwidth = 1, fill="orange", color="black")
ggplot(aes(Unhealthy.for.Sensitive.Groups.Days), data=air_quality_data) +
geom_histogram(binwidth = 0.5, fill="blue", color="black")
ggplot(aes(Unhealthy.Days), data=air_quality_data) +
geom_histogram(binwidth = 1, fill="yellow", color="black")
ggplot(aes(Very.Unhealthy.Days), data=air_quality_data) +
geom_histogram(binwidth = 0.5, fill="purple", color="black")
ggplot(aes(Hazardous.Days), data=air_quality_data) +
geom_histogram(binwidth = 0.5, fill="red", color="black")
state_air_quality_data <- air_quality_data %>%
group_by(State, County) %>%
summarize(mean_AQI = mean(Good.Days/Days.with.AQI),
n=n()) %>%
arrange(County, State, mean_AQI)
